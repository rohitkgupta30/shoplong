{
  "name": "recently_viewed_products",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "user_id": {
      "type": "string",
      "required": true
    },
    "product_id": {
      "type": "string",
      "required": true
    },
    "brand_id": {
      "type": "string",
      "required": true
    },
    "category_id": {
      "type": "string",
      "required": true
    },
    "created": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "userDetail": {
      "type": "belongsTo",
      "model": "users",
      "foreignKey": "user_id"
    },
    "productDetail": {
      "type": "belongsTo",
      "model": "products",
      "foreignKey": "product_id"
    },
    "brandDetail": {
      "type": "belongsTo",
      "model": "brands",
      "foreignKey": "brand_id"
    },
    "categoryDetail": {
      "type": "belongsTo",
      "model": "categories",
      "foreignKey": "category_id"
    }
  },
  "acls": [],
  "methods": {}
}
